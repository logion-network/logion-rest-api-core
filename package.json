{
  "name": "@logion/rest-api-core",
  "version": "0.1.0-1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logion-network/logion-rest-api-core.git"
  },
  "keywords": [
    "logion",
    "authentication"
  ],
  "author": {
    "name": "Logion Team",
    "email": "info@logion.network",
    "url": "https://logion.network/"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && yarn lint && tsc --project ./tsconfig.json",
    "lint": "yarn eslint src/**",
    "test": "jasmine --config=jasmine.json",
    "integration-test": "yarn run setup-test-db && jasmine --config=jasmine-integration.json ; yarn run teardown-test-db",
    "setup-test-db": "docker run -d --rm --name logion-test-db -e POSTGRES_PASSWORD=secret -p 5432:5432 logionnetwork/logion-postgres:latest",
    "teardown-test-db": "docker stop logion-test-db",
    "coverage": "nyc yarn run test"
  },
  "dependencies": {
    "@logion/authenticator": "^0.1.0-3",
    "dinoloop": "^2.4.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.2.1",
    "express-oas-generator": "^1.0.44",
    "inversify": "^6.0.1",
    "mongoose": "^6.5.0",
    "mongoose-to-swagger": "^1.4.0",
    "openapi-typescript": "5.1.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.6.2",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.20.0",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "moq.ts": "^9.0.2",
    "nyc": "^15.1.0",
    "pg": "^8.8.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "node-fetch": "2.6.7"
  },
  "packageManager": "yarn@3.2.3"
}
